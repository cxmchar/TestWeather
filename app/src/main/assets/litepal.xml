<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--
    	数据库名，例如：
    	<dbname value="demo" />
    -->
    <dbname value="cool_weather" />

    <!--
    	数据库版本号，例如：
    	<version value="1" />
    -->
    <version value="1" />

    <!--
    	指定映射模型，下面会讲到，例如：
    	<list>
    		<mapping class="com.test.model.Reader" />
    		<mapping class="com.test.model.Magazine" />
    	</list>
    -->
    <list>
        <mapping class="demo.cxm.com.testweather.db.City" />
        <mapping class="demo.cxm.com.testweather.db.County" />
        <mapping class="demo.cxm.com.testweather.db.Province" />
    </list>
</litepal>